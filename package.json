{
  "name": "curriculum",
  "version": "16.0.0",
  "private": true,
  "description": "Визитка разработчика",
  "devDependencies": {
    "autoprefixer": "9.5.x",
    "browser-sync": "2.26.x",
    "gulp": "^4.0.2",
    "gulp-less": "^4.0.1",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "8.0.x",
    "gulp-sourcemaps": "2.6.x",
    "stylelint": "9.10.x"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/less/**/*.less\" --syntax less",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "gulp css",
    "start": "gulp start"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
